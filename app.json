{
  "expo": {
    "name": "eJoi",
    "slug": "ejoi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/logos/eJoi_logos-01.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/logos/eJoi_logos-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#f20a64"
    },
    "fonts": [
      "./public/Tipografia/Copia de Apis-Black.ttf",
      "./public/Tipografia/Copia de Apis-BlackItalic.ttf",
      "./public/Tipografia/Copia de Apis-Bold.ttf",
      "./public/Tipografia/Copia de Apis-BoldItalic.ttf",
      "./public/Tipografia/Copia de Apis-ExtraBold.ttf",
      "./public/Tipografia/Copia de Apis-ExtraBoldItalic.ttf",
      "./public/Tipografia/Copia de Apis-Italic.ttf",
      "./public/Tipografia/Copia de Apis-Light.ttf",
      "./public/Tipografia/Copia de Apis-LightItalic.ttf",
      "./public/Tipografia/Copia de Apis-Medium.ttf",
      "./public/Tipografia/Copia de Apis-MediumItalic.ttf",
      "./public/Tipografia/Copia de Apis-Regular.ttf"
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ejoi.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/logos/eJoi_logos-01.png",
        "backgroundColor": "#f20a64"
      },
      "package": "com.ejoi.app"
    },
    "web": {
      "favicon": "./public/logos/eJoi_logos-01.png",
      "bundler": "metro",
      "output": "static"
    },
    "scheme": "ejoi",
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}
